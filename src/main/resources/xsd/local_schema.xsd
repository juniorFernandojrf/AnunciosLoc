<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:tns="http://local.soap.xml"
	targetNamespace="http://local.soap.xml"
	elementFormDefault="qualified"
>

  <!-- Definindo o tipo para o cabeçalho -->
  <xs:complexType name="HeaderType">
    <xs:sequence>
      <!-- Token de sessão -->
      <xs:element name="AuthToken" type="xs:string"/>
    </xs:sequence>
  </xs:complexType>
  
  <!-- Definição do elemento "user" -->
  <xs:element name="AddLocalRequest">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Header" type="tns:HeaderType" />
        <xs:element name="Body">
          <xs:complexType>
            <xs:sequence>
              <!-- nome -->
              <xs:element name="nome" type="xs:string" />
              <!-- latitude -->
              <xs:element name="latitude" type="xs:string" />
              <!-- longetude -->
              <xs:element name="longetude" type="xs:string" />
              <!-- mensagem -->
              <xs:element name="mensagem" type="xs:string" />
              <!-- status -->
              <xs:element name="status" type="xs:string" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

    <!-- Definição do elemento "user" -->
  <xs:element name="AddLocalResponse">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Header" type="tns:HeaderType" />
        <xs:element name="Body">
          <xs:complexType>
            <xs:sequence>
              <!-- nome -->
              <xs:element name="nome" type="xs:string" />
              <!-- latitude -->
              <xs:element name="latitude" type="xs:string" />
              <!-- longetude -->
              <xs:element name="longetude" type="xs:string" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>


</xs:schema>
